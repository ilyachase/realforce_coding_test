{
  "name": "ilyachase/realforce_coding_test",
  "description": "Coding test for Realforce",
  "authors": [
    {
      "name": "Ilya Chase",
      "email": "ilya.chase@yandex.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.3",
    "moneyphp/money": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "fzaninotto/faker": "^1.8",
    "symfony/var-dumper": "^4.3"
  },
  "scripts": {
    "test": "phpunit --bootstrap vendor/autoload.php --testdox tests"
  }
}
